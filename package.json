{
  "name": "quuli-new",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prisma:inline": "cp ./node_modules/.prisma/client/*.js ./node_modules/@prisma/client",
    "prisma:migrate": "prisma migrate deploy && npm run prisma:inline",
    "prisma:migrate:dev": "prisma migrate dev && npm run prisma:inline",
    "prisma:generate": "prisma generate && npm run prisma:inline"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.45.0",
    "flowbite-svelte-icons": "^1.5.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prisma": "^5.12.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.0",
    "@auth/sveltekit": "^1.0.0",
    "@prisma/client": "^5.12.1",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.13"
  }
}
